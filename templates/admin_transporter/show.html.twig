{% extends 'base.html.twig' %}

{% block title %}Transporter{% endblock %}

{% block body %}

    <h1>Transporter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transporter.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ transporter.title }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ transporter.content }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ transporter.price }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_transporter_index') }}">Retour</a>

    <a href="{{ path('app_admin_transporter_edit', {'id': transporter.id}) }}">Modifier</a>

    {{ include('admin_transporter/_delete_form.html.twig') }}
{% endblock %}
